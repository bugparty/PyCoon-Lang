fun sieve(int n) {
    bool primes[n+1];
    int i;
    int j;
    for (i = 2; i <= n; i = i + 1) {
        primes[i] = true;
    }
    for (i = 2; i * i <= n; i = i + 1) {
        if (primes[i]) {
            for (j = i * i; j <= n; j = j + i) {
                primes[j] = false;
            }
        }
    }
    for (i = 2; i <= n; i = i + 1) {
        if (primes[i]) {
            print(i);
        }
    }
}

sieve(30);